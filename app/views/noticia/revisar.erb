<div class="modal-content">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
    <h4 class="modal-title">Noticia del <%= @noticia.created_at  %></h4>
  </div>
  <div class="modal-body">
    <form role="form">
      <div class="form-body">
        <div class="form-group">
          <label class="control-label">Autor</label>
          <div class="">
            <i class="tooltips" data-original-title="You look OK!" data-container="body"></i>
            <div class="contenido">
            <%= @noticia.administrador.nombres.to_s() %> <%= @noticia.administrador.apellidos.to_s() %>
            </div>
          </div>
        </div>
        <div class="form-group">
          <label class="control-label">TÃ­tulo</label>
          <div class="">
            <i class="tooltips" data-original-title="Email address" data-container="body"></i>
            <div class="contenido">
            <%= @noticia.titulo.to_s() %>
          </div>
          </div>
        </div>
        <div class="form-group">
          <label class="control-label">Contenido</label>
          <div class="">
            <i class="tooltips" data-original-title="You look OK!" data-container="body"></i>
            <div class="contenido">
            <%= @noticia.noticia.html_safe %>
            </div>
          </div>
        </div>

      </div></form>
  </div>

  <div class="modal-footer">
